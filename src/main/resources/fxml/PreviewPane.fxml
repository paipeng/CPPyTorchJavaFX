<?xml version="1.0" encoding="UTF-8"?>

<?import com.paipeng.cppytorch.view.PreviewPane?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<fx:root type="PreviewPane" xmlns="http://javafx.com/javafx/17"
         xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="com.paipeng.cppytorch.view.PreviewPane">
    <AnchorPane prefHeight="640.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/17"
                xmlns:fx="http://javafx.com/fxml">
        <StackPane fx:id="stackPane">
            <children>
                <ImageView fx:id="previewImageView" fitHeight="640.0" fitWidth="640.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </StackPane>
    </AnchorPane>
</fx:root>